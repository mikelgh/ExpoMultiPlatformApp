{
  "name": "expo-multiplatform-app-template",
  "version": "1.0.0",
  "description": "A truly cross-platform app template - One codebase, three platforms (Web, Android, Windows)",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/expo-multiplatform-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/expo-multiplatform-app/issues"
  },
  "homepage": "https://github.com/yourusername/expo-multiplatform-app#readme",
  "keywords": [
    "expo",
    "react-native",
    "react-native-web",
    "electron",
    "cross-platform",
    "multiplatform",
    "android",
    "web",
    "windows",
    "typescript",
    "template"
  ],
  "main": "index.ts",
  "electronMain": "electron.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "electron": "start-electron.bat",
    "electron:dev": "node_modules\\.bin\\electron.cmd electron.js",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "expo": "~54.0.25",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.3"
  },
  "private": true,
  "build": {
    "appId": "com.expomultiplatformapp",
    "productName": "MultiPlatformApp",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": ["nsis", "portable"],
      "icon": "assets/icon.png"
    }
  }
}
